{
  "version": 2,
  "projects": {
    "@typescript-eslint/ast-spec": {
      "root": "packages/ast-spec",
      "type": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": ["packages/ast-spec/dist"],
          "options": {
            "cwd": "packages/ast-spec",
            "parallel": false,
            "commands": [
              "npx tsc -b tsconfig.build.json",
              "npx api-extractor run --local"
            ]
          }
        },
        "clean": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "packages/ast-spec",
            "parallel": false,
            "commands": [
              "npx tsc -b tsconfig.build.json --clean",
              "npx rimraf dist",
              "npx rimraf _ts3.4",
              "npx rimraf coverage",
              "npx rimraf .rollup.cache"
            ]
          }
        },
        "typecheck": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "packages/ast-spec",
            "parallel": false,
            "commands": ["npx tsc -p tsconfig.json --noEmit"]
          }
        }
      }
    },
    "@typescript-eslint/eslint-plugin": {
      "root": "packages/eslint-plugin",
      "type": "library"
    },
    "@typescript-eslint/eslint-plugin-internal": {
      "root": "packages/eslint-plugin-internal",
      "type": "library"
    },
    "@typescript-eslint/eslint-plugin-tslint": {
      "root": "packages/eslint-plugin-tslint",
      "type": "library"
    },
    "@typescript-eslint/experimental-utils": {
      "root": "packages/experimental-utils",
      "type": "library"
    },
    "@typescript-eslint/parser": {
      "root": "packages/parser",
      "type": "library"
    },
    "@typescript-eslint/scope-manager": {
      "root": "packages/scope-manager",
      "type": "library"
    },
    "@typescript-eslint/shared-fixtures": {
      "root": "packages/shared-fixtures",
      "type": "library"
    },
    "@typescript-eslint/types": {
      "root": "packages/types",
      "type": "library",
      "targets": {
        "build": {
          "dependsOn": [
            {
              "target": "copy-ast-spec",
              "projects": "self"
            },
            {
              "target": "build",
              "projects": "dependencies"
            }
          ],
          "executor": "@nrwl/workspace:run-commands",
          "outputs": ["packages/types/dist"],
          "options": {
            "cwd": "packages/types",
            "parallel": false,
            "commands": [
              "npx tsc -b tsconfig.build.json",
              "npx downlevel-dts dist _ts3.4/dist"
            ]
          }
        },
        "copy-ast-spec": {
          "dependsOn": [
            {
              "target": "build",
              "projects": "dependencies"
            }
          ],
          "executor": "@nrwl/workspace:run-commands",
          "outputs": ["packages/types/src/ast-spec.ts"],
          "options": {
            "cwd": "packages/types",
            "parallel": false,
            "commands": ["npx ts-node -s ./tools/copy-ast-spec.ts"]
          }
        },
        "clean": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "packages/types",
            "parallel": false,
            "commands": [
              "npx tsc -b tsconfig.build.json --clean",
              "npx rimraf dist",
              "npx rimraf _ts3.4",
              "npx rimraf coverage"
            ]
          }
        },
        "typecheck": {
          "dependsOn": [
            {
              "target": "copy-ast-spec",
              "projects": "self"
            },
            {
              "target": "build",
              "projects": "dependencies"
            }
          ],
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "packages/types",
            "parallel": false,
            "commands": ["npx tsc -p tsconfig.json --noEmit"]
          }
        },
        "generate-lib": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "packages/types",
            "parallel": false,
            "commands": [
              "../../node_modules/.bin/ts-node --files --transpile-only ../scope-manager/tools/generate-lib.ts"
            ]
          }
        }
      }
    },
    "@typescript-eslint/typescript-estree": {
      "root": "packages/typescript-estree",
      "type": "library"
    },
    "@typescript-eslint/visitor-keys": {
      "root": "packages/visitor-keys",
      "type": "library"
    }
  }
}
